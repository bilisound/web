{
    "extends": "@electron-toolkit/tsconfig/tsconfig.node.json",
    "include": [
        "electron.vite.config.*",
        "src/main/*",
        "src/main/*/**.ts",
        "src/preload/*",
        "build/*",
        ".eslintrc.cjs",
    ],
    "compilerOptions": {
        "composite": true,
        "types": [
            "electron-vite/node"
        ],
        "noUnusedLocals": false,
        "noUnusedParameters": false
    }
}
